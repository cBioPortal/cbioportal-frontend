        window.oncoKBApiUrl = "oncokb.org/api/v1";
    </script>
    <script src="reactapp/common.bundle.js"></script>
    <link href="reactapp/prefixed-bootstrap.min.css" rel="stylesheet"></link>
</head>
<body class="cbioOnePageApp">
