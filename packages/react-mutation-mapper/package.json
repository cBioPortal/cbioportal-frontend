{
  "name": "react-mutation-mapper",
  "version": "0.1.5",
  "description": "Generic Mutation Mapper",
  "author": "cBioPortal",
  "license": "GNU Affero General Public License v3.0",
  "repository": "cBioPortal/react-mutation-mapper",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "typings": "dist/index.d.ts",
  "styles": "dist/styles.css",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "updateGenomeNexusAPI": "yarn run fetchGenomeNexusAPI && yarn run buildGenomeNexusAPI",
    "fetchGenomeNexusAPI": "curl https://www.genomenexus.org/v2/api-docs | json | grep -v basePath | grep -v termsOfService | grep -v host > src/generated/GenomeNexusAPI-docs.json && curl https://www.genomenexus.org/v2/api-docs?group=internal | json | grep -v basePath | grep -v termsOfService | grep -v host > src/generated/GenomeNexusAPIInternal-docs.json",
    "buildGenomeNexusAPI": "node scripts/generate-api.js src/generated GenomeNexusAPI GenomeNexusAPIInternal",
    "updateOncoKbAPI": "yarn run fetchOncoKbAPI && yarn run buildOncoKbAPI",
    "fetchOncoKbAPI": "curl https://www.oncokb.org/api/v1/v2/api-docs | json | grep -v basePath | grep -v termsOfService | grep -v host > src/generated/OncoKbAPI-docs.json",
    "buildOncoKbAPI": "node scripts/generate-api.js src/generated OncoKbAPI"
  },
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "cbioportal-frontend-commons": "^0.0.8",
    "classnames": "^2.2.6",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "mobxpromise": "github:cbioportal/mobxpromise#v1.0.2",
    "react-collapse": "^4.0.3",
    "react-motion": "^0.5.2",
    "react-table": "^6.10.0",
    "seamless-immutable": "^7.1.4",
    "superagent": "^5.0.5"
  },
  "peerDependencies": {
    "mobx": "^3.0.0 || ^4.0.0 || ^5.0.0",
    "mobx-react": "^4.0.0 || ^5.0.0",
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^4.2.0",
    "@types/chai": "^4.1.7",
    "@types/classnames": "^2.2.7",
    "@types/jest": "^23.1.5",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^5.2.6",
    "@types/react": "^16.8.10",
    "@types/react-collapse": "^4.0.2",
    "@types/react-dom": "^16.8.3",
    "@types/react-table": "^6.8.1",
    "@types/rollup-plugin-commonjs": "^9.2.0",
    "@types/rollup-plugin-node-resolve": "^4.0.0",
    "@types/rollup-plugin-sourcemaps": "^0.4.2",
    "@types/seamless-immutable": "^7.1.9",
    "@types/sinon": "^7.0.12",
    "@types/superagent": "^4.1.1",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "fs": "^0.0.1-security",
    "gh-pages": "^1.2.0",
    "json": "^9.0.6",
    "mobx": "^3.1.7",
    "mobx-react": "^4.1.3",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "postcss-url": "^8.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts-ts": "^3.1.0",
    "request": "^2.88.0",
    "rollup": "^1.7.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-url": "^2.2.0",
    "sinon": "^7.3.2",
    "swagger-js-codegen": "git+https://github.com/cBioPortal/swagger-js-codegen.git#0362f4a1e2d116ad6dffc36e4b57cdfbc93956cf",
    "typescript": "^3.4.1"
  },
  "files": [
    "dist"
  ]
}
